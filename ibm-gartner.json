              },
              "X-Rate-Limit-Limit" : {
                "schema" : {
                  "type" : "string"
                },
                "example" : "10.00"
              },
              "X-Rate-Limit-Request-Remote-Addr" : {
                "schema" : {
                  "type" : "string"
                },
                "example" : "172.17.0.1:56796"
              },
